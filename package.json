{
  "name": "vue-slick-ts",
  "version": "0.3.0",
  "description": "Vue/Nuxt component for slick-carousel with full Typescript",
  "copyright": "Copyright Â© 2023-present Efcolipt",
  "main": "./dist/vue-slick-ts.umd.js",
  "module": "./dist/vue-slick-ts.es.js",
  "unpkg": "./dist/vue-slick-ts.umd.js",
  "jsdelivr": "./dist/vue-slick-ts.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/vue-slick-ts.es.js"
      },
      "require": "./dist/vue-slick-ts.cjs.js",
      "default": "./dist/vue-slick-ts.es.js"
    },
    "./types": "./dist/types/index.d.ts",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./dist/style.css": "./dist/style.css",
    "./dist/style-theme.css": "./dist/style-theme.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=16.19.0",
    "npm": ">=9.6.4"
  },
  "scripts": {
    "dev": "vite",
    "lint:fix": "eslint --fix --ext .ts,vue ./",
    "build": "vite build && vue-tsc --project tsconfig.production.json",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Efcolipt/vue-slick-ts.git"
  },
  "keywords": [
    "slick",
    "carousel",
    "vue",
    "nuxt-slick",
    "slick-ts",
    "vue-slick",
    "vue3-slick"
  ],
  "author": "Ivan Baykov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Efcolipt/vue-slick-ts/issues"
  },
  "homepage": "https://github.com/Efcolipt/vue-slick-ts#readme",
  "peerDependencies": {
    "vue": "^3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.42.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "jquery": ">=1.8.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "release-it": "^15.10.5",
    "slick-carousel": "^1.8.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.16.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      }
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,vue}": "eslint --fix"
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        2,
        "always",
        130
      ]
    },
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
