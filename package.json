{
  "name": "vue-slick-ts",
  "version": "0.4.1",
  "main": "./dist/vue-slick-ts.umd.js",
  "module": "./dist/vue-slick-ts.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/vue-slick-ts.umd.js",
      "default": "./dist/vue-slick-ts.es.js"
    },
    "./dist/css/style.css": {
      "import": "./dist/css/style.css",
      "require": "./dist/css/style.css"
    },
    "./dist/css/style-theme.css": {
      "import": "./dist/css/style-theme.css",
      "require": "./dist/css/style-theme.css"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=14.19.3"
  },
  "author": "Baykov Ivan <efcolipt@gmail.com> (https://github.com/Efcolipt/)",
  "description": "Vue/Nuxt component for slick-carousel with full Typescript",
  "copyright": "Copyright Â© 2023-present Efcolipt",
  "license": "MIT",
  "homepage": "https://github.com/Efcolipt/vue-slick-ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Efcolipt/vue-slick-ts.git"
  },
  "keywords": [
    "slick",
    "carousel",
    "vue",
    "nuxt-slick",
    "slick-ts",
    "vue-slick",
    "vue3-slick"
  ],
  "bugs": {
    "url": "https://github.com/Efcolipt/vue-slick-ts/issues"
  },
  "scripts": {
    "dev": "vite",
    "lint:fix": "eslint --fix --ext .ts,vue ./",
    "build": "vite build && vue-tsc --project tsconfig.production.json",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.42.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "jquery": ">=1.8.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "release-it": "^15.10.5",
    "slick-carousel": "^1.8.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.16.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      }
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,vue}": "eslint --fix"
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        2,
        "always",
        130
      ]
    },
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}